<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="za.nmu.keyboardprototype.KeyboardController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <GridPane alignment="CENTER" hgap="5.0" vgap="5.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="48.0" minWidth="10.0" prefWidth="48.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="49.0" minHeight="10.0" prefHeight="49.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="11.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
          <Button fx:id="qButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="Q" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         <Button fx:id="wButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="W" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         <Button fx:id="eButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="E" GridPane.columnIndex="3" GridPane.rowIndex="1" />
          <Button fx:id="rButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="R" GridPane.columnIndex="4" GridPane.rowIndex="1" />
          <Button fx:id="tButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="T" GridPane.columnIndex="5" GridPane.rowIndex="1" />
          <Button fx:id="yButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="Y" GridPane.columnIndex="6" GridPane.rowIndex="1" />
          <Button fx:id="uButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="U" GridPane.columnIndex="7" GridPane.rowIndex="1" />
          <Button fx:id="iButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="I" GridPane.columnIndex="8" GridPane.rowIndex="1" />
          <Button fx:id="oButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="O" GridPane.columnIndex="9" GridPane.rowIndex="1" />
          <Button fx:id="pButton" onKeyPressed="#onButtonClick" onKeyReleased="#onButtonRelease" prefHeight="48.0" prefWidth="48.0" text="P" GridPane.columnIndex="10" GridPane.rowIndex="1" />
      </children>
   </GridPane>
   <Label fx:id="text" />
</VBox>
